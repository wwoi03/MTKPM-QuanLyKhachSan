@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <title>Phố đèn đỏ</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/admin/assets/img/favicon.png">
    <link rel="stylesheet" href="~/admin/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/admin/assets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/admin/assets/plugins/fontawesome/css/all.min.css">
    <link rel="stylesheet" href="~/admin/assets/css/feathericon.min.css">
    <link rel="stylehseet" href="https://cdn.oesmith.co.uk/morris-0.5.1.css">
    <link rel="stylesheet" href="~/admin/assets/plugins/morris/morris.css">
    <link rel="stylesheet" href="~/admin/assets/css/base.css">
    <link rel="stylesheet" href="~/admin/assets/css/style.css">

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/admin/assets/assets2/plugins/fontawesome-free/css/all.min.css">
    
    <!-- Theme style -->
    <link rel="stylesheet" href="~/admin/assets/assets2/dist/css/adminlte.min.css">
    
    <!-- Font Awesome Stylesheet -->
    <link href="~/css/fontawesome-free-6.5.1-web/css/all.css" rel="stylesheet">
    
    <!-- fullCalendar -->
    <script src="~/admin/assets/plugins/fullcalendar-scheduler-6.1.10/dist/index.global.js"></script>
    <link rel="stylesheet" href="~/admin/assets/plugins/fullcalendar-scheduler-6.1.10/custom-fullcalendar.css">

    <!-- DateTimePicker -->
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" type="text/css" href="~/lib/datetimepicker-master/jquery.datetimepicker.css">

    <!-- jQuery -->
    <script src="~/admin/assets/js/jquery-3.5.1.min.js"></script>
    <script src="~/admin/assets/js/popper.min.js"></script>
    <script src="~/admin/assets/js/bootstrap.min.js"></script>
    <script src="~/admin/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/admin/assets/plugins/raphael/raphael.min.js"></script>
    <script src="~/admin/assets/plugins/morris/morris.min.js"></script>
    <script src="~/admin/assets/js/chart.morris.js"></script>
    <script src="~/admin/assets/js/script.js"></script>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/admin/assets/assets2/plugins/jquery/jquery.min.js"></script>
    <script src="~/admin/assets/assets2/plugins/jquery-ui/jquery-ui.min.js"></script>
</head>

<body>
    <div class="main-wrapper">
        <!-- Toast -->
        <div id="custom-toast"></div>

        <div class="header">
            <div class="header-left" style="border-right: 1px solid #E5E5E5">
                <a href="index.html" class="logo"> <img src="~/admin/assets/img/hotel_logo.png" width="50" height="70" alt="logo"> <span class="logoclass">HOTEL DEL LUNA</span> </a>
                <a href="index.html" class="logo logo-small"> <img src="~/admin/assets/img/hotel_logo.png" alt="Logo" width="30" height="30"> </a>
            </div>

            <a href="javascript:void(0);" id="toggle_btn" style="margin-left: 0;"> <i class="fe fe-text-align-left"></i> </a>

			<span class="title-pape">@ViewData["PapeTitle"]</span>

            @if (@ViewBag.openAddBookRoom != null ? @ViewBag.openAddBookRoom : false) {
                <div class="btn-page">
                    <i class="fa-solid fa-file-circle-plus"></i>
				    <span>Thêm mới</span>
                </div>

                <div class="btn-page btn-page-excel">
                    <i class="fa-solid fa-file-excel"></i>
				    <span>Nhập excel</span>

                    <form class="form-excel" method="post" asp-action="ImportFileExcel" asp-controller="BookRoom" enctype="multipart/form-data">
						<h1>Thêm excel</h1>
						<input type="file" name="excelFile" accept=".xlsx, .xls" />

						<div class="form-excel-btn">
							<input id="form-excel-input" type="submit" value="Thêm" />
                        </div>
                    </form>
                </div>
            }
			
            <a class="mobile_btn" id="mobile_btn"> 
                <i class="fas fa-bars"></i> 
            </a>
        </div>
        <div class="sidebar" id="sidebar" style="border-right: 1px solid #E5E5E5">
            <div class="sidebar-inner slimscroll">
                <div id="sidebar-menu" class="sidebar-menu">
                    <ul>
                        <li class="sidebar-page-item"> 
                            <a class="sidebar-page-link" href="/Admin/AdminHome/Index">
                                <i class="fas fa-tachometer-alt"></i> 
                                <span>Trang chính</span>
                            </a> 
                        </li>

                        <li class="list-divider"></li>

                        <li class="sidebar-page-item active"> 
                            <a class="sidebar-page-link" href="/Admin/AdminBooking/Index" id="click-book-room">
                                <i class="fas fa-suitcase"></i> 
                                <span>Đặt phòng</span>
                            </a> 
                        </li>

                        <li class="sidebar-page-item"> 
                            <a class="sidebar-page-link" href="/Admin/AdminRentCheckOut/Index">
                                <i class="fas fa-key"></i> 
                                <span>Thuê - trả phòng</span>
                            </a> 
                        </li>

                        <li class="sidebar-page-item"> 
                            <a class="sidebar-page-link" href="/Admin/AdminSystemManager/Index">
                                <i class="fas fa-cog"></i> 
                                <span>Quản lý hệ thống</span>
                            </a>
                        </li>

                        <li class="sidebar-page-item"> 
                            <a class="sidebar-page-link" href="/Admin/AdminAccount/Index">
                                <i class="fas fa-user"></i> 
                                <span>Tài khoản</span>
                            </a> 
                        </li>
                    </ul>
                </div>
            </div>
        </div>

        <div class="page-wrapper">
            <div class="w-100">
                @RenderBody()
            </div>
        </div>
    </div>
    
    <!-- Bootstrap -->
    <script src="~/admin/assets/assets2/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/admin/assets/assets2/dist/js/demo.js"></script>
    <!-- SweetAlert2 -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <!-- DateTimePicker -->
    <script src="~/lib/datetimepicker-master/build/jquery.datetimepicker.full.min.js"></script>

    <script src="~/admin/assets/js/admin-main.js"></script>
    <script src="~/admin/assets/js/admin-rent-check-out.js"></script>
    <script src="~/admin/assets/js/admin-account.js"></script>
    <script src="~/admin/assets/js/admin-room.js"></script>
</body>
</html>